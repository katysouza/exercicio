algoritmo "jogo_adivinhacao"
// Função :
// Autor :
// Data : 06/04/2024
// Seção de Declarações 
var
numero : inteiro
numero2 : inteiro

inicio
numero2 <- 7
escreva("digite um número: ")
leia(numero)
se numero = numero2 entao
   escreva("você acertou!")
senao se numero < numero2 entao
   escreva("o numero é maior")
senao entao
   escreva("o numero é menor")


fimse

fimalgoritmo